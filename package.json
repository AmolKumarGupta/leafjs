{
  "name": "leafjs",
  "version": "1.0.0",
  "description": "frontend library for UI",
  "source": "src/leaf.js",
  "main": "dist/leaf.js",
  "module": "dist/leaf.mjs",
  "standalone": "dist/leaf-min.js",
  "targets": {
    "main": {
      "source": "src/leaf.js"
